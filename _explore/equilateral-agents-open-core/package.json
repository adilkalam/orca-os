{
  "name": "equilateral-agents-open-core",
  "version": "2.5.0",
  "description": "22 Self-Learning AI Agents - Build Institutional Knowledge That Compounds Over Time - Production-Ready Orchestration Framework",
  "main": "index.js",
  "exports": {
    ".": "./index.js",
    "./protocols/MinimalMCPServer": "./equilateral-core/protocols/MinimalMCPServer.js",
    "./protocols/AgentCommunicationBus": "./equilateral-core/protocols/AgentCommunicationBus.js",
    "./protocols/ProtocolCompatibilityLayer": "./equilateral-core/protocols/ProtocolCompatibilityLayer.js"
  },
  "files": [
    "index.js",
    "equilateral-core/**/*.js",
    "agent-packs/**/*.js",
    "workflows/**/*.js",
    "LICENSE",
    "README.md",
    "CHANGELOG.md",
    "AGENT_INVENTORY.md",
    "AI-INTEGRATION.md",
    "BYOL-SETUP.md"
  ],
  "keywords": [
    "ai-agents",
    "multi-agent",
    "orchestration",
    "automation",
    "aws",
    "bedrock",
    "github-actions",
    "development-tools",
    "claude-code",
    "llm",
    "byol"
  ],
  "author": "HappyHippo.ai",
  "license": "MIT",
  "homepage": "https://github.com/Equilateral-AI/equilateral-agents-open-core",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Equilateral-AI/equilateral-agents-open-core.git"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "test": "jest --testPathPattern=test/ --passWithNoTests",
    "prepublishOnly": "npm test",
    "start": "node demo-all-agents-enhanced.js",
    "dev": "nodemon index.js",
    "agents": "node equilateral-core/cli/orchestrator-cli.js",
    "demo": "node demo-all-agents-enhanced.js",
    "demo:basic": "node demo-all-agents.js",
    "ai-demo": "node examples/ai-enhanced-workflow.js",
    "github-demo": "node examples/github-integration.js",
    "aws-demo": "node examples/aws-bedrock-demo.js",
    "wow": "node examples/test-analyze-demo.js",
    "wow:advanced": "echo 'Advanced features available in commercial version' && node examples/test-analyze-demo.js",
    "wow:form": "node examples/smart-form-filler.js",
    "playground": "node examples/playground.js",
    "tutorial": "echo 'Tutorial coming soon - try playground instead!' && npm run playground",
    "benchmark": "echo 'Benchmark coming soon - try wow instead!' && npm run wow",
    "compare": "node demo-all-agents-enhanced.js --compare",
    "workflow:security": "node workflows/run-security-review.js",
    "workflow:quality": "node workflows/run-code-quality.js",
    "workflow:deploy": "node -e \"require('./workflows/deployment-pipeline-workflow.js')\"",
    "workflow:fullstack": "node -e \"require('./workflows/full-stack-development-workflow.js')\"",
    "workflow:infrastructure": "node -e \"require('./workflows/infrastructure-validation-workflow.js')\"",
    "demo:background": "node examples/background-execution-demo.js",
    "demo:memory": "node examples/agent-memory-example.js",
    "memory:stats": "node -e \"const {SimpleAgentMemory} = require('./index.js'); const fs = require('fs'); const dirs = fs.readdirSync('.agent-memory'); dirs.forEach(d => { const m = new SimpleAgentMemory(d); console.log(m.getStats()); });\"",
    "harvest": "node scripts/harvest-knowledge.js",
    "harvest:weekly": "node scripts/harvest-knowledge.js --since=7d",
    "harvest:monthly": "node scripts/harvest-knowledge.js --since=30d"
  },
  "dependencies": {
    "aws-sdk": "^2.1691.0",
    "axios": "^1.6.0",
    "commander": "^11.1.0",
    "discord.js": "^14.22.1",
    "dotenv": "^16.3.1",
    "playwright": "^1.55.1"
  },
  "optionalDependencies": {
    "better-sqlite3": "^9.2.2"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "nodemon": "^3.0.2"
  },
  "bugs": {
    "url": "https://github.com/Equilateral-AI/equilateral-agents-open-core/issues"
  },
  "directories": {
    "lib": "equilateral-core",
    "example": "examples"
  },
  "publishConfig": {
    "access": "public"
  }
}
